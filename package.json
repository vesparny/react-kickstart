{
  "name": "react-kickstart",
  "version": "6.1.4",
  "description": "just another react + webpack boilerplate.",
  "private": true,
  "author": "Alessandro Arnodo <alessandro@arnodo.net> (https://alessandro.arnodo.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vesparny/react-kickstart/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vesparny/react-kickstart.git"
  },
  "homepage": "https://github.com/vesparny/react-kickstart",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --coverage",
    "dev": "node devServer",
    "start": "cross-env NODE_ENV=production node prodServer",
    "bump": "standard-version",
    "prebuild": "npm run build:clean",
    "build:clean": "rimraf build",
    "build": "npm run test && npm run lint && cross-env NODE_ENV=production webpack --config webpack.config.prod.js --color -p --progress",
    "gh-pages": "gh-pages --dist build",
    "release": "npm run test && npm run lint && npm run bump && npm run build && git push --follow-tags origin master && npm run gh-pages",
    "precommit": "lint-staged",
    "lint": "standard"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "jest",
      "expect",
      "it",
      "test",
      "describe"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --semi false --single-quote",
      "standard --fix",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "**/*.{js}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "roots": [
      "src"
    ]
  },
  "dependencies": {
    "glamor": "^2.20.25",
    "glamorous": "^3.14.0",
    "prop-types": "^15.5.9",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1",
    "reflexbox": "^2.2.3",
    "sanitize.css": "^5.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.3",
    "jest-cli": "^20.0.1",
    "jest-glamor-react": "^1.3.0",
    "json-loader": "^0.5.4",
    "lint-staged": "^3.4.1",
    "lodash": "^4.13.1",
    "prettier": "^1.3.1",
    "react-addons-test-utils": "^15.5.1",
    "redbox-react": "^1.3.6",
    "rimraf": "^2.6.1",
    "standard": "^10.0.2",
    "standard-version": "^4.0.0",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
